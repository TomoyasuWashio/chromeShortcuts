{
  "name": "Tab and window shortcuts",
  "description": "Shortcuts for tab and window actions for Windows and Linux.",
  "version": "1.20",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "icons":{
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "permissions": ["sessions"],
  "content_security_policy": "default-src 'self' https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js",
  "manifest_version": 2
}